{
  "cli": {
    "version": ">= 12.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "production": {
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "simulator": {
      "ios": {
        "simulator": true,
        "resourceClass": "m-medium"
      }
    },
    "development-simple": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "buildConfiguration": "Debug"
      }
    },
    "distribution": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "buildConfiguration": "Release",
        "distribution": "internal"
      }
    },
    "no-apple-id": {
      "ios": {
        "resourceClass": "m-medium",
        "buildConfiguration": "Debug",
        "simulator": false,
        "distribution": "internal"
      }
    },
    "standalone": {
      "ios": {
        "resourceClass": "m-medium",
        "buildConfiguration": "Release",
        "distribution": "internal",
        "simulator": false
      }
    }
  },
  "submit": {
    "production": {}
  }
}
